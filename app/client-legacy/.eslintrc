{
    "extends": ["eslint:recommended", "plugin:react/recommended"],
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    plugins: [
      "react"
    ],
    "rules": {
        "semi": [1, "never"],
    },
    "env": {
      "browser": true,
    },
}
